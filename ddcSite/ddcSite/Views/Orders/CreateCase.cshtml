@{
    ViewBag.Title = "CreateCase";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form id="commintForm" method="get" action="">

    <h2>Prescription</h2>
    <div class="form-horizontal">
        <h4>Case: <label class="control-label-static" id="caseName"></label></h4>
        <hr />
        <div class="row">
            <div class="col-sm-4">
                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">Full patient name</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <input class="form-control txt" id="txtPatientName" type="text" min="5" name="Fullpatientname" minle required />
                        </div>
                    </div>
                </div>
            </div>
            @*<div class="col-sm-4">
                    <div class="form-group">
                        <div class="row">
                            <label class="control-label-static col-md-6">Patient last name</label>
                        </div>
                        <div class="row">
                            <div class="col-md-10">
                                <input class="form-control txt" id="txtPatientLastName" required />
                            </div>
                        </div>
                    </div>
                </div>*@
            <div class="col-sm-4">

                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">Age</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <input class="form-control" type="number" id="Age" name="Age" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4">
                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">Due date</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <div class='input-group date'>
                                <input type='text' class="form-control fecha" id="DateDelivery"  />
                                @*<span class="input-group-addon"  style="height: 35px">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </span>*@
                                @*<input class="form-control " id="DateDelivery" tooltip="aleast 1 day after apointment" required name="DateDelivery" />*@
                                <br />
                                <span style="color: red" class="control-label-static">Please 1 day before appointment</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">Creation Case</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <input class="form-control" id="DateCreation" readonly />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">Gender</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <select id="Gender" class="form-control">
                                <option></option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4" style="display: none">
                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">Location</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <input class="form-control" id="Location" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4" style="display: none">
                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">Score</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <input class="form-control" id="score" readonly />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4" style="display: none">
                <div class="form-group">
                    <div class="row">
                        <label class="control-label-static col-md-6">IdClient</label>
                    </div>
                    <div class="row">
                        <div class="col-md-10">
                            <input class="form-control" id="IdClient" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @*panel products added, Table*@

        <hr title="products" />
        <h4>Restauration Types</h4>
        <div class="row">
            <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#myModal" id="btnAddItem">
                Add
            </button>
            <button type="button" class="btn btn-primary btn-sm " id="btnRemove">
                Remove
                @*<span class="glyphicon glyphicon-minus"></span>*@
            </button>
            <table class="table table-striped table-bordered" id="table_id">
                <thead>
                    <tr>
                        <th>
                            id
                        </th>
                        <th>
                            items
                        </th>
                        <th>
                            Teeth
                        </th>
                        <th>
                            Product
                        </th>

                        <th>
                            Note
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>

        <div class="row" style="display: none">
            <div class="form-group">
                <div class="row">
                    <label class="control-label-static col-md-6">Coupon</label>
                </div>
                <div class="row">
                    <div class="col-md-10">
                        <input class="form-control" id="Coupon" />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3"></div>
            <div id="partialView" class="col-md-6"></div>
            <div class="col-md-3"></div>
        </div>

        @*@Html.RenderPartial("Drag_drop_File")*@

        @*<hr />*@

        <div class="form-group">
            <div class="row">
                <p id="validation"></p>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <button id="btnCreateOrder" type="button" class="btn btn-primary btn-sm">Create Case</button>
                </div>
            </div>
        </div>
        @*panel shoping cart*@


    </div>

</form>
<div id="divFixed" style="display: none;position: fixed; background: white; right: 50px; top: 150px; width: 350px" class="opaco">
    <div class="modal-content">
        <div class="modal-header">
            <h4 class="modal-title">Detail order</h4>
        </div>
        <div class="row">
            <div id="orderItems"></div>
        </div>
        <div class="row" style="display: none">
            <div class="form-group">
                <div class="col-sm-6 col-sm-offset-1">
                    <label class="control-label-static col-md-6">Name</label>
                </div>
                <div class="col-sm-4" style="display: block">
                    <input class="form-control" id="Name" readonly />
                </div>
            </div>
        </div>
        <div class="row" style="display: block">
            <div class="form-group">
                <div class="col-sm-6 col-sm-offset-1">
                    <label class="control-label-static col-md-6">Price</label>
                </div>
                <div class="col-sm-4">
                    <input class="form-control" id="Price" readonly />
                </div>
            </div>
        </div>
        <div class="row" style="display: block">
            <div class="form-group">
                <div class="col-sm-6 col-sm-offset-1">
                    <label class="control-label-static col-md-10">Tax</label>
                </div>
                <div class="col-sm-4">
                    <input class="form-control" id="PriceTax" readonly />
                </div>

            </div>
        </div>
        <div class="row" style="display: block">
            <div class="form-group">
                <div class="col-sm-6 col-sm-offset-1">
                    <label class="control-label-static col-md-10">Total price</label>
                </div>
                <div class="col-sm-4">
                    <input class="form-control" id="PriceTotal" readonly />
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" id="bntTransparency">Transparency</button>
        </div>

    </div>
</div>
@*panel modal show item to add to list*@
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                @*<button type="button" class="glyphicon-refresh" id="btnClean">
                        <span aria-hidden="true">&times;</span>
                    </button>*@
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
