        function insertAfter(referenceNode, newNode) {
            referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
        }

        var validateInformation = function() {
            var form = document.getElementById("commintForm");
            if ($(".ErrorRequired").is(":visible") == true) {
                $(".ErrorRequired").remove();
            }
            var span;
            var node;
            if (ldetails.length==0) {
                var btn=document.getElementById("btnAddItem");
                span = document.createElement("span");
                span.setAttribute( 'class', 'ErrorRequired' );
                span.setAttribute( 'id', 'spanErrorItems' );
                span.setAttribute( 'style', 'color:red' );
                node = document.createTextNode("Required Items!!!!");
                span.appendChild(node);
                //var input = form.elements[i]
                insertAfter(btn, span);
                alertify.error("Required items",5 );
            }
            for(var i=0; i < form.elements.length; i++) {
                var inputDate = form.elements[i].id;
                if (inputDate=="DateDelivery") {
                    var startDate = $('#DateCreation').val().replace('-','/');
                    var endDate = $('#DateDelivery').val().replace('-','/');

                    if(startDate > endDate){
                        span = document.createElement("span");
                        span.setAttribute( 'class', 'ErrorRequired' );
                        span.setAttribute( 'style', 'color:red' );
                        node = document.createTextNode("Date must be later!!!!");
                        span.appendChild(node);
                        input = form.elements[i];
                        insertAfter(input, span);
                    }
                }
                if(form.elements[i].value === "" && form.elements[i].hasAttribute('required')){
                    span = document.createElement("span");
                    span.setAttribute( 'class', 'ErrorRequired' );
                    span.setAttribute( 'style', 'color:red' );
                    node = document.createTextNode("Required Field!!!!");
                    span.appendChild(node);
                    input = form.elements[i];
                    insertAfter(input, span);
                }


            }
            if ($(".ErrorRequired").is(":visible") == true) {
                alertify.error("Required fields",5 );
                return false;
            }
            else {
                return true;
            }
        };

